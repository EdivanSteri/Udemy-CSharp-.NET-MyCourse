{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "Default": "Data Source=Data/MyCourse.db"
  },

  "Courses": {
    "PerPage": 10,
    "Order": {
      "By": "Rating",
      "Ascending": false,
      "Allow": [ "Title", "Rating", "CurrentPrice" ]
    }
  }

}
